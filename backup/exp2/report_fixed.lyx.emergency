\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
Q_{a} & = & CV_{0}-A\exp\left(-\frac{t}{\tau}\right)\\
I_{a}(t) & = & \frac{V_{0}}{R_{R}}-B\exp\left(-\frac{t}{\tau}\right)\\
A & = & \frac{2CV_{0}}{e^{\pm\frac{T}{2t}}+1}\\
B & = & \frac{2V_{0}}{R_{T}\left(e^{\pm\frac{T}{2t}}+1\right)}\\
+ & for & -\frac{T}{2}<t<0\\
- & for & 0<t<\frac{T}{2}
\end{eqnarray*}

\end_inset


