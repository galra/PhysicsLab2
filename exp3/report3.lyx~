\begin_inset Formula $R_{E}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
R_{E}\equiv\frac{V_{p}}{I_{P}}=\left(\frac{\frac{N_{P}}{N_{S}}\cdot V_{S}}{\frac{N_{S}}{N_{P}}\cdot I_{S}}\right)=\left(\frac{N_{P}}{N_{S}}\right)^{2}\left(\frac{V_{S}}{I_{S}}\right)=\left(\frac{N_{P}}{N_{S}}\right)^{2}R_{L}
\]

\end_inset

כאשר 
\begin_inset Formula $R_{L}$
\end_inset

 מוגדר להיות 
\begin_inset Formula $R_{L}=\frac{V_{S}}{I_{S}}$
\end_inset

 התנגדות היציאה השקולה
\end_layout

\begin_layout Standard
ולכן נקבל כי 
\begin_inset Formula 
\begin{equation}
R_{E}=a^{2}\cdot R_{L}\label{eq:Re}
\end{equation}

\end_inset

 כאשר 
\begin_inset Formula $a=\frac{N_{P}}{N_{S}}$
\end_inset


